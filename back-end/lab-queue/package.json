{
  "name": "lab-queue",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "db:diagram": "typeorm-uml src/ormconfig.ts",
    "db:makemigration": "ts-node node_modules/typeorm/cli.js migration:generate -d migration -n",
    "db:migrationrevert": "ts-node node_modules/typeorm/cli.js migration:revert ",
    "db:migrate:test": "ts-node node_modules/typeorm/cli.js -c test migration:run",
    "db:migrate:clear": "rm -r test.migration -f && rm -r migration -f",
    "db:makemigration:test": "ts-node node_modules/typeorm/cli.js migration:generate -c test -d migration  -n migration",
    "db:migrationrevert:deploy": "ts-node node_modules/typeorm/cli.js migration:revert -с deploy",
    "db:migrate:deploy": "ts-node node_modules/typeorm/cli.js migration:run -с deploy",
    "typeorm": "ts-node node_modules/typeorm/cli.js",
    "build": "npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prebuild && nest start",
    "start:dev": "npm run prebuild && nest start --watch",
    "start:debug": "npm run prebuild && nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run prebuild && jest",
    "test:watch": "npm run prebuild && jest --watch",
    "test:cov": "npm run prebuild && jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:run:e2e": "jest --config ./test/jest-e2e.json",
    "test:db:create": "sudo -u postgres psql -f ./test/createtestdb.SQL",
    "test:db:fill": "sudo -u postgres psql queue_test -f ./test/filltestdb.SQL",
    "test:db:drop": "sudo -u postgres psql -f ./test/droptestdb.SQL",
    "test:db:prepare": "npm run db:migrate:clear && npm run test:db:create && npm run db:makemigration:test && npm run db:migrate:test && npm run test:db:fill",
    "test:e2e": "npm run prebuild && npm run test:db:prepare &&  npm run test:clearCache && npm run test:run:e2e && npm run db:migrate:clear",
    "test:clearCache": "jest --clearCache",
    "prebuild": "rm -rf src/shared/front-back-end && node ../../scripts/fill-dist-folder.js",
    "compodoc": "npx compodoc -p tsconfig.json src -s"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/typeorm": "^7.1.5",
    "core-util-is": "^1.0.2",
    "cors": "^2.8.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "recursive-copy": "^2.0.11",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "typeorm": "^0.2.32",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.2.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm-uml": "^1.6.2",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
